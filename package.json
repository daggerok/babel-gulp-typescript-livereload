{
  "name": "babel-gulp-typescript-livereload",
  "version": "1.0.0",
  "description": "gulp node typescript tsify babe es6 browserify babelify local-web-server bootstrap gulp-server-livereload",
  "main": "index.js",
  "local-web-server": {
    "port": 3000,
    "forbid": [
      "*.json",
      "*.ts",
      "*.md",
      "*.yml",
      "*.babel.js",
      ".gitignore"
    ],
    "no-cache": true
  },
  "babel": {
    "presets": ["es2015"]
  },
  "scripts": {
    "gulp": "gulp",
    "pregulp": "npm i",
    "clean": "rm -rf dist",
    "test": "npm run gulp",
    "prestart": "npm test",
    "start": "npm run gulp watch & ws"
  },
  "author": "Maksim Kostromin",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "bootstrap": "^3.3.6",
    "browserify": "^13.0.0",
    "colors": "^1.1.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-livereload": "^3.8.1",
    "local-web-server": "^1.1.1",
    "require-dir": "^0.3.0",
    "tsify": "^0.13.2",
    "typescript": "^1.8.2",
    "vinyl-source-stream": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/babel-gulp-typescript-livereload.git"
  },
  "keywords": [
    "es6",
    "gulp",
    "babel",
    "gulp-babel",
    "browserify",
    "tsify",
    "babelify",
    "typescript",
    "bootstrap",
    "local-web-server",
    "gulp-livereload"
  ],
  "bugs": {
    "url": "https://github.com/daggerok/babel-gulp-typescript-livereload/issues"
  },
  "homepage": "https://github.com/daggerok/babel-gulp-typescript-livereload#readme"
}
